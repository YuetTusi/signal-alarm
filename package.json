{
	"name": "signal-alarm",
	"private": true,
	"version": "0.0.1",
	"main": "dist/src/main/index.js",
	"scripts": {
		"go": "electron .",
		"predev": "rimraf dist/**/*",
		"dev": "tsc src/main/index.ts --outDir dist && cross-env NODE_ENV=development concurrently \"vite\" \"pnpm run go\"",
		"prebuild": "rimraf dist/**/*",
		"build": "vite build && tsc src/main/index.ts --outDir dist",
		"preview": "vite preview",
		"dist": "pnpm run build && electron-builder"
	},
	"build": {
		"appId": "4c9e34def3049532",
		"productName": "信号哨兵长时检测系统",
		"copyright": "Copyright © 2023 北京万盛华通科技有限公司",
		"directories": {
			"buildResources": "static"
		},
		"asar": true,
		"asarUnpack": [
			"dist/assets/**/*",
			"dist/src/main/**/*",
			"dist/index.html"
		],
		"files": [
			"dist/**/*",
			"icon.png",
			"package.json"
		],
		"extraResources": [],
		"win": {
			"target": [
				"zip"
			],
			"icon": "icon.png",
			"publisherName": "yuet",
			"artifactName": "${productName}.${ext}",
			"requestedExecutionLevel": "requireAdministrator"
		}
	},
	"dependencies": {
		"@ant-design/icons": "^5.0.1",
		"antd": "^5.3.0",
		"dayjs": "^1.11.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.8.2",
		"styled-components": "^5.3.8",
		"zustand": "^4.3.6"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.3.0",
		"@fortawesome/free-brands-svg-icons": "^6.3.0",
		"@fortawesome/free-regular-svg-icons": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.3.0",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@types/node": "^18.14.6",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@types/styled-components": "^5.1.26",
		"@vitejs/plugin-react": "^3.1.0",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"electron": "^23.1.2",
		"electron-builder": "^23.6.0",
		"typescript": "^4.9.3",
		"vite": "^4.1.0",
		"vite-plugin-optimizer": "^1.4.2"
	}
}
